{
    "name": "wooyg/mapcme",
    "description": "The Mapcme Framework.",
    "keywords": ["framework", "mapcme", "cms"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "phpdocumentor/phpdocumentor": "*",
        "erusev/parsedown": "*",
        "erusev/parsedown-extra": "*",
        "phpmailer/phpmailer": "*",
        "bshaffer/oauth2-server-php": "*",
        "gabordemooij/redbean": "dev-master",
        "auth0/auth0-php": "^5.6"
    },
    "require-dev": {
